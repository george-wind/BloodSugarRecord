<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="george.ni.medicare">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="george.ni.medicare.activity.MainActivity"
            android:screenOrientation="portrait">

        </activity>


        <activity
            android:name="george.ni.medicare.activity.StartActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.AllBloodSugarRecordsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AllBloodPressureRecordsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CheckBloodSugarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CheckBloodPressureActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>